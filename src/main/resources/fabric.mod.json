{
  "schemaVersion": 1,
  "id": "plateau",
  "version": "${version}",
  "name": "PlateauLib",
  "description": "common code that I often using when create new mods",
  "authors": [
    "MrSypz"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/plateau/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.sypztep.plateau.client.PlateauClient"
    ],
    "main": [
